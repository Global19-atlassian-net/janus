{
    "builds": [
        {
            "name": "{virus}",
            "virus": ["zika", "ebola"]
        },
        {
            "name": "{virus}_{lineage}_{segment}_{resolution}",
            "virus": ["flu"],
            "lineage": ["h3n2", "h1n1pdm"],
            "segment": ["ha"],
            "resolution": ["3y", "6y"],
            "prepare": "--sequences fauna/data/{lineage}.fasta --titers fauna/data/{lineage}_hi_titers.tsv",
            "process": "--no_raxml --no_mut_freqs --no_tree_freqs"
        },
        {
            "name": "{virus}_{lineage}_{segment}_{resolution}_{passage}_{assay}",
            "virus": ["flu"],
            "lineage": ["h3n2", "h1n1pdm"],
            "segment": ["ha"],
            "resolution": ["3y", "6y"],
            "passage": ["cell", "egg"],
            "assay": ["hi", "fra"],
            "prepare": "--sequences fauna/data/{lineage}.fasta --titers fauna/data/{lineage}_cdc_{assay}_{passage}_titers.tsv",
            "process": "--no_raxml"
        }
    ]
}
