{
    "builds": [
        {
            "name": "{virus}",
            "virus": ["zika", "ebola"]
        },
        {
            "name": "avian_h7n9_{segments}",
            "virus": "avian",
            "lineage": "h7n9",
            "segments": ["pb2", "pb1", "pa", "ha", "np", "na", "mp", "ns"],
            "prepare": "--sequences ../../../fauna/data/h7n9_{segments}.fasta"
        },
        {
            "name": "{virus}_{lineage}_{segment}_{resolution}",
            "virus": ["flu"],
            "lineage": ["h3n2", "h1n1pdm", "vic", "yam"],
            "segment": ["ha"],
            "resolution": ["3y", "6y", "12y"],
            "prepare": "--sequences ../../../fauna/data/{lineage}.fasta --titers ../../../fauna/data/{lineage}_hi_titers.tsv --complete_frequencies"
        },
        {
            "name": "{virus}_{lineage}_{segment}_{resolution}_{passage}_{assay}",
            "virus": ["flu"],
            "lineage": ["h3n2", "h1n1pdm", "vic", "yam"],
            "segment": ["ha"],
            "resolution": ["3y", "6y", "12y"],
            "passage": ["cell", "egg"],
            "assay": ["hi", "fra"],
            "prepare": "--sequences ../../../fauna/data/{lineage}.fasta --titers ../../../fauna/data/{lineage}_cdc_{assay}_{passage}_titers.tsv --complete_frequencies",
            "process": "--titers_export"
        }
    ]
}
